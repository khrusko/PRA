@using UI.Models
@using BLL.Projection
@model  LoanSearchVM
@{
  ViewBag.Title = "Kontrolna ploča";
  var user = Session["user"] as UserProjection;
}

<h2>Kontrolna ploča</h2>

@if (user.IsAdmin) {
  @Html.Partial(partialViewName: "_AdminDashboard", model: Model)
}
else {
  @Html.Partial(partialViewName: "_UserDashboard", model: Model.Loans)
}